<h1>Wassup <%= @user.username %></h1>

<h3>My factions:</h3>
<ul>
<% @user.liked_factions.each do |faction| %>
  <li style="font-size: 20px;"><%= faction.name %></li>
  <ul>
    <li><%= link_to "About", faction_path(faction.id) %></li>
    <li><%= link_to "Strategies", faction_strategies_path(faction.id) %></li>
    <br/>
  </ul>
<% end %>
</ul>
<%= link_to 'Edit Factions', profile_factions_edit_path %>
